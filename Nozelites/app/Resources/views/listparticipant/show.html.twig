{% extends 'base.html.twig' %}

{% block body %}
    <h1>Listparticipant</h1>

    <table>
        <tbody>
            <tr>
                <th>Ide</th>
                <td>{{ listparticipant.ide }}</td>
            </tr>
            <tr>
                <th>Idm</th>
                <td>{{ listparticipant.idm }}</td>
            </tr>
            <tr>
                <th>Etatp</th>
                <td>{{ listparticipant.etatp }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('listparticipant_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('listparticipant_edit', { 'ide': listparticipant.ide }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
