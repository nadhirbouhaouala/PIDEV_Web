<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title></title>
</head>
<body>
    <h1>Groupes list</h1>

    <table>
        <thead>
            <tr>
                <th>Idgroupe</th>
                <th>Titre</th>
                <th>Description</th>
                <th>Autorisation</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for groupe in groupes %}
            <tr>
                <td>{{ groupe.idGroupe }}</td>
                <td>{{ groupe.titre }}</td>
                <td>{{ groupe.description }}</td>
                <td>{{ groupe.autorisation }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

</body>
</html>
